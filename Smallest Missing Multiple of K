class Solution {
public:
    int missingMultiple(vector<int>& nums, int k) {
        unordered_map<int,bool> numPresent;
        for (int i=0; i<nums.size(); i++) {
            numPresent[nums[i]]=true;
        }
        int num=k;
        while (numPresent[num]) {
            num+=k;
        }
        return num;
    }
};
