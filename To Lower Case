class Solution {
    public String toLowerCase(String s) {
        int n=s.length();
        char[] arr=s.toCharArray();
        for (int i=0; i<n; i++) {
            if (arr[i]>='A' && arr[i]<='Z') {
                int pos=arr[i]-'A';
                arr[i]=(char)('a'+pos);
            }
        }
        return new String(arr);
    }
}
