import java.util.HashMap;
import java.util.Map;

class Solution {
    public int maxFreqSum(String s) {
        Map<Character, Integer> letterFreq = new HashMap<>();
        for (int i = 0; i < s.length(); i++) {
            char letter = s.charAt(i);
            letterFreq.put(letter, letterFreq.getOrDefault(letter, 0) + 1);
        }
        int maxVowelFreq = 0, maxConsFreq = 0;
        for (char letter = 'a'; letter <= 'z'; letter++) {
            int freq = letterFreq.getOrDefault(letter, 0);

            if (letter == 'a' || letter == 'e' || letter == 'i' || letter == 'o' || letter == 'u') {
                if (maxVowelFreq < freq) {
                    maxVowelFreq = freq;
                }
            } else {
                if (maxConsFreq < freq) {
                    maxConsFreq = freq;
                }
            }
        }
        return maxVowelFreq + maxConsFreq;
    }
}
