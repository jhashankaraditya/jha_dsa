#define ll long long
class Solution {
public:
    int longestSubsequence(vector<int>& nums) {
        ll int xorVal=0;
        for (ll int i=0; i<nums.size(); i++) {
            ll int num=nums[i];
            xorVal=(xorVal^num);
        }
        if (xorVal!=0) {
            return nums.size();
        }
        sort(nums.begin(),nums.end());
        if (nums[nums.size()-1]) {
            return nums.size()-1;
        }
        return 0;
    }
};
