class Solution {
public:
    long long removeZeros(long long n) {
        string num=to_string(n);
        string newNum;
        for (int i=0; i<num.size(); i++) {
            if (num[i]!='0') {
                newNum.push_back(num[i]);
            }
        }
        long long finalNum=stoll(newNum);
        return finalNum;
    }
};
