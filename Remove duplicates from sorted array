class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        unordered_map<int,bool> numsFreq;
        vector<int> distNums;
        for (int i=0; i<nums.size(); i++) {
            if (numsFreq.find(nums[i])==numsFreq.end()) {
                distNums.push_back(nums[i]);
                numsFreq[nums[i]]=true;
            }
        }
        for (int i=0; i<distNums.size(); i++) {
            nums[i]=distNums[i];
        }
        return distNums.size();
    }
};
