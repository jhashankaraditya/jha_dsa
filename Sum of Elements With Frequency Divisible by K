class Solution {
public:
    int sumDivisibleByK(vector<int>& nums, int k) {
        unordered_map<int,int> numsFreq;
        for (int i=0; i<nums.size(); i++) {
            numsFreq[nums[i]]++;
        }
        int sum=0;
        for (auto it:numsFreq) {
            int freq=it.second;
            if (freq%k==0) {
                sum+=(freq*it.first);
            }
        }
        return sum;
    }
};
