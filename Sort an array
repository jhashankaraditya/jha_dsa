class Solution {
public:
    vector<int> sortArray(vector<int>& nums) {
        map<int,int> nums_freq;
        for (int i=0; i<nums.size(); i++) {
            nums_freq[nums[i]]++;
        }
        vector<int> nums_sorted;
        for (auto it:nums_freq) {
            for (int i=0; i<it.second; i++) {
                nums_sorted.push_back(it.first);
            }
        }
        return nums_sorted;
    }
};
