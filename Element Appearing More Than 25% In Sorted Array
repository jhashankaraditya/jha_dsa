class Solution {
public:
    int findSpecialInteger(vector<int>& arr) {
        int curr_freq=0, ans=-1;
        for (int i=0; i<arr.size(); i++) {
            curr_freq++;
            if (curr_freq*4>arr.size()) {
                ans=arr[i];
                break;
            }
            if (i==arr.size()-1||arr[i+1]!=arr[i]) {
                curr_freq=0;
            }
        }
        return ans;
    }
};
