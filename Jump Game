class Solution {
public:
    bool canJump(vector<int>& nums) {
        vector<bool> marked(nums.size(),0);
        marked[0]=true;
        int farthest_index=0;
        for (int i=0; i<nums.size(); i++) {
            if (i>farthest_index) {
                return false;
            }
            farthest_index=max(farthest_index,i+nums[i]);
        }
        return true;
    }
};
