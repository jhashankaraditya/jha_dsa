class Solution {
public:
    vector<vector<int>> flipAndInvertImage(vector<vector<int>>& image) {
        int n=image.size();
        vector<vector<int>> ans(n,vector<int>(n,0));
        for (int i=n-1; i>=0; i--) {
            for (int j=n-1; j>=0; j--) {
                ans[i][n-1-j]=1-image[i][j];
            }
        }
        return ans;
    }
};
