class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int low=0, high=nums.size()-1, ans=-1;
        while (low<=high) {
            int mid=low+(high-low)/2;
            if (nums[mid]<target) {
                low=mid+1;
            }
            else {
                ans=mid;
                high=mid-1;
            }
        }
        if (ans==-1) {
            return nums.size();
        }
        return ans;
    }
};
