class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        char ans;
        if (letters[letters.size()-1]-target<=0)
            ans=letters[0];
        else {
            for (int i=0; i<letters.size(); i++) {
                if (letters[i]-target>0) {
                    ans=letters[i];
                    break;
                }
            }
        }return ans;
    }
};
