class Solution {
public:
    int maxDistinct(string s) {
        int substrings=0;
        unordered_map<char,bool> letterMarked;
        for (int i=0; i<s.size(); i++) {
            if (!letterMarked[s[i]]) {
                letterMarked[s[i]]=true;
                substrings++;
            }
        }
        return substrings;
    }
};
