[⚠️ Suspicious Content] class Solution {
public:
    vector<string> validateCoupons(vector<string>& code, vector<string>& businessLine, vector<bool>& isActive) {
        unordered_map<string,vector<string>> businessLine_coupons;
        unordered_map<char,bool> validCouponChars;
        for (char letter='a'; letter<='z'; letter++) {
            validCouponChars[letter]=true;
        }
        for (char letter='A'; letter<='Z'; letter++) {
            validCouponChars[letter]=true;
        }
        for (char letter='0'; letter<='9'; letter++) {
            validCouponChars[letter]=true;
        }
        validCouponChars['_']=true;
        for (int i=0; i<code.size(); i++) {
            if (code[i].size()==0) {
                continue;
            }
            bool couponValid=true;
            for (int j=0; j<code[i].size(); j++) {
                if (!validCouponChars[code[i][j]]) {
                    couponValid=false;
                    break;
                }
            }
            if (!couponValid) {
                continue;
            }
            if (!(businessLine[i]=="electronics" || businessLine[i]=="grocery" || businessLine[i]=="pharmacy" || businessLine[i]=="restaurant")) {
                continue;
            }
            if (isActive[i]==false) {
                continue;
            }
            businessLine_coupons[businessLine[i]].push_back(code[i]);
        }
        vector<string> ans;
        sort(businessLine_coupons["electronics"].begin(),businessLine_coupons["electronics"].end());
        sort(businessLine_coupons["grocery"].begin(),businessLine_coupons["grocery"].end());
        sort(businessLine_coupons["pharmacy"].begin(),businessLine_coupons["pharmacy"].end());
        sort(businessLine_coupons["restaurant"].begin(),businessLine_coupons["restaurant"].end());
        for (int i=0; i<businessLine_coupons["electronics"].size(); i++) {
            ans.push_back(businessLine_coupons["electronics"][i]);
        }
        for (int i=0; i<businessLine_coupons["grocery"].size(); i++) {
            ans.push_back(businessLine_coupons["grocery"][i]);
        }
        for (int i=0; i<businessLine_coupons["pharmacy"].size(); i++) {
            ans.push_back(businessLine_coupons["pharmacy"][i]);
        }
        for (int i=0; i<businessLine_coupons["restaurant"].size(); i++) {
            ans.push_back(businessLine_coupons["restaurant"][i]);
        }
        return ans;
    }
};
