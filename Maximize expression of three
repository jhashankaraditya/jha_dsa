class Solution {
public:
    int maximizeExpressionOfThree(vector<int>& nums) {
        int maxSum=INT_MIN, n=nums.size();
        for (int i=0; i<n; i++) {
            for (int j=0; j<n; j++) {
                for (int k=0; k<n; k++) {
                    if (i!=j && j!=k && k!=i) {
                        maxSum=max(maxSum,nums[i]+nums[j]-nums[k]);
                    }
                }
            }
        }
        return maxSum;
    }
};
